{
  "manifest_version": 3,
  "name": "MongoDB Extension",
  "version": "1.0",
  "action": {
    "default_popup": "templates/index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "mongodb://*/*"
  ]
}
